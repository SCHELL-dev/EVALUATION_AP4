<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Membre">

    <TextView
        android:id="@+id/txtPageMembre"
        android:layout_width="278dp"
        android:layout_height="52dp"
        android:layout_marginStart="65dp"
        android:layout_marginTop="62dp"
        android:layout_marginEnd="65dp"
        android:text="@string/txtPageMembre"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnRetourMembre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="155dp"
        android:layout_marginTop="404dp"
        android:layout_marginEnd="164dp"
        android:text="@string/btnRetour"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtPageMembre" />

    <ListView
        android:id="@+id/membre_listview"
        android:layout_width="409dp"
        android:layout_height="402dp"
        tools:layout_editor_absoluteX="1dp"
        tools:layout_editor_absoluteY="115dp" />
</androidx.constraintlayout.widget.ConstraintLayout>